.box>input{
  width: 90%;
  height: 100%;
  margin-left: 5%;
  border-radius: 20px;
  text-indent: 30px;
}
.box{
  width: 100%;
  height: 46px;
  position: relative;
}
.box>image{
  width: 20px;
  position: absolute;
  top: 14px;
  left: 26px;
  height: 20px;
}
.place-holder{
  letter-spacing: 1px;
}
.img>image{
  width: 120px;
  height: 120px;
  overflow: hidden;
}
.list_box{
  display: flex;
  /* justify-content: space-around; */
  margin-bottom: 12px;
  padding: 0 12px;
}
.img{
  position: relative;
}
.img_text{
  position: absolute;
  bottom: 10px;
  left: 10px;
  color: #fff;
}
.list{
  margin-left: 12px;
  padding-top: 4px;
}
.list_txt{
  font-size: 12px;
}
.list_font{
  line-height: 30px;
}
